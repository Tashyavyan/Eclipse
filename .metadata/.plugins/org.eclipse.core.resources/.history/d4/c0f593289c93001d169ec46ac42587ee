  }
  
  int numT = 100;
  
  if(numT > 0 && numT <= 5) {
	  System.out.println("10% discount");
  }
  if(numT > 5 && numT <= 10) {
	  System.out.println("20% discount");
  }
  if(numT > 20) {
	  System.out.println("30% discount");
  
	}
if(numT > 0 && numT<= 5) {
	System.out.println("10% discount");
}
else if(numT > 5 && numT <= 10) {
	System.out.println("20% discount");
}
else if(numT > 75) {
	System.out.println("30% discount");
}
	}
