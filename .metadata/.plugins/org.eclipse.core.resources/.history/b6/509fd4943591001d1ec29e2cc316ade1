package practice;

public class day1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
System.out.println("hello suman");

int a=25;
int b=15;
	System.out.println(a+b);
	System.out.println(a-b);
	System.out.println(a*b);
	System.out.println(a/b);
	System.out.println(a%b);
	
	calculator (10,15);
	}
	static void calculator(int a, int b) {
		
		System.out.println(a+b);
		System.out.println(a+b);
		System.out.println(a-b);
		System.out.println(a*b);
		System.out.println(a/b);
		System.out.println(a%b);
	}
}
